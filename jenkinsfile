pipeline {
    agent any

    stages 
    {
        stage('Build') 
        {
            steps 
            {
                echo 'Building Calculator'
                --docker build -t calculator . 
                build 'calculator-pipeline'
            }
        }
        stage('Test') 
        {
            steps {
                echo 'Testing Calculator'
            }
        }
        stage('Deploy') 
        {
            steps {
                echo 'Deploying Calculator'
            }
        }
    }
}
